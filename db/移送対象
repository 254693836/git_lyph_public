/*lyphtest lyphtest lyphtest lyphtest lyphtest lyphtest lyphtest*/
SELECT 
   E071.OBJ_NAME,
   E070.AS4DATE,
   E070.AS4TIME,
   E070.AS4USER,
   E070.TRKORR,
   E071.LOCKFLAG
FROM E071 
   INNER JOIN E070 ON E071.TRKORR = E070.TRKORR
WHERE 
   E071.OBJ_NAME LIKE '%8700%'
ORDER BY 
    E071.OBJ_NAME,
    E070.AS4DATE DESC,
    E070.AS4TIME

